{
	"name": "frontend",
	"version": "0.1.1",
	"private": true,
	"homepage": "./",
	"main": "./public/electron.js",
	"author": "Eins <eins.yeager@gmail.com>",
	"description": "My fantastic Electron app",
	"build": {
		"appId": "com.electron.myapp",
		"productName": "My Electron App",
		"files": [
			"build/**/*",
			"node_modules/**/*"
		],
		"directories": {
			"buildResources": "public"
		},
		"mac": {
			"target": "dmg"
		},
		"win": {
			"target": "nsis"
		},
		"linux": {
			"target": "deb"
		}
	},
	"dependencies": {
		"@reduxjs/toolkit": "^2.2.3",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"axios": "^1.6.8",
		"electron-is-dev": "^3.0.1",
		"js-cookie": "^3.0.5",
		"moment": "^2.30.1",
		"react": "^18.3.0",
		"react-dom": "^18.3.0",
		"react-redux": "^9.1.0",
		"react-router-dom": "^6.22.3",
		"react-scripts": "5.0.1",
		"rxjs": "^7.8.1"
	},
	"scripts": {
		"start": "craco start",
		"build": "craco build",
		"test": "craco test",
		"eject": "craco eject",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
		"electron:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on tcp:127.0.0.1:3000 && electronmon .\"",
		"electron:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
		"electron:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
		"electron:linux": "npm run build && electron-builder --linux tar.xz -c.extraMetadata.main=build/electron.js"
	},
	"browserslist": {
		"production": [
			"last 1 electron version"
		],
		"development": [
			"last 1 electron version"
		]
	},
	"devDependencies": {
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"@craco/craco": "^7.1.0",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"electron": "^30.0.2",
		"electron-builder": "^24.13.3",
		"electronmon": "^2.0.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"prettier": "^3.2.5",
		"sass": "^1.75.0",
		"wait-on": "^7.2.0"
	}
}